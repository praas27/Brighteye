<Window x:Class="Brighteye.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Brighteye"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="btnOrderNummers" Content="Order nummers" HorizontalAlignment="Left" Margin="50,144,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.871,0.732" Click="btnOrderNummers_Click"/>
        <Button x:Name="btnFillNummers" Content="Fill nummers" HorizontalAlignment="Left" Margin="50,104,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.612,0.547" Click="btnFillNummers_Click"/>

    </Grid>
</Window>
